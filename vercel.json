{
  "version": 2,
  "buildCommand": "npm run build",
  "outputDirectory": "dist/freshcart",
  "devCommand": "npm start",
  "installCommand": "npm install",
  "framework": "angular",
  "routes": [
    {
      "src": "/assets/(.*)",
      "headers": {
        "cache-control": "public, max-age=31536000, immutable"
      }
    },
    {
      "src": "/(.*\\.(js|css|woff2?|ttf|eot|svg|png|jpg|jpeg|gif|ico|json))",
      "headers": {
        "cache-control": "public, max-age=31536000, immutable"
      }
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}
